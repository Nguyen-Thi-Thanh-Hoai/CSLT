def Input():
    n = int(input("n="))
    global L,x
    L=[]
    for i in range(n):
        x=int(input(""))
        L.append(x)
    x=int(input("x="))
    return L, x

def FirstAndLast(L):
    result = [L[0], L[-1]]
    print(result,sep="")
    return result

def Search(L, x):
    if x in L:
        print("True")
        return True
    else:
        print("False")
        return False

Input()
FirstAndLast(L)
Search(L, x)
